{
    "name": "dachcom-digital/jobs",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore Jobs API",
    "keywords": [
        "pimcore",
        "jobs"
    ],
    "homepage": "https://github.com/dachcom-digital/pimcore-jobs",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "JobsBundle\\": "src/JobsBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "JobsBundle\\JobsBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "^6.0",
        "symfony/symfony": "^4|^5",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^8.0"
    },
    "conflict": {
        "symfony/monolog-bundle": "3.6.0"
    },
    "suggest": {
        "facebook/graph-sdk": "^5.4",
        "dachcom-digital/schema": "^1.0",
        "dachcom-digital/seo": "^1.0"
    }
}
